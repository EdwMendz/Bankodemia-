<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".verificacionIdentidad.VerificacionIdentidad2">

    <ImageView
        android:id="@+id/imageViewIdentidad2"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginTop="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bk" />

    <TextView
        android:id="@+id/textViewIdentidad2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="19dp"
        android:text="@string/identidad"
        android:fontFamily="@font/poppins_500"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewIdentidad2" />

    <TextView
        android:id="@+id/textViewTipoDocumento"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/poppins_500"
        android:lineSpacingExtra="5sp"
        android:text="@string/textViewIdentidad3"
        android:textColor="@color/negro"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewIdentidad2" />

    <TextView
        android:id="@+id/tvTipoDocumento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="28dp"
        android:clickable="true"
        android:focusable="true"
        android:text="@string/ine"
        android:fontFamily="@font/poppins_400"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTipoDocumento" />

    <TextView
        android:id="@+id/textViewDocMigratorio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:clickable="true"
        android:focusable="true"
        android:text="@string/documentoMigratorio"
        android:fontFamily="@font/poppins_400"
        app:layout_constraintStart_toStartOf="@+id/tvTipoDocumento"
        app:layout_constraintTop_toBottomOf="@+id/tvTipoDocumento" />

    <TextView
        android:id="@+id/textViewPasaporte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:clickable="true"
        android:focusable="true"
        android:text="@string/pasaporte"
        android:fontFamily="@font/poppins_400"
        app:layout_constraintStart_toStartOf="@+id/textViewDocMigratorio"
        app:layout_constraintTop_toBottomOf="@+id/textViewDocMigratorio" />

    <TextView
        android:id="@+id/textViewProteccionDoc"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="64dp"
        android:clickable="true"
        android:focusable="true"
        android:fontFamily="@font/poppins_500"
        android:text="@string/textViewIdentidad4"
        android:textAlignment="center"
        android:textColor="@color/lila"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>